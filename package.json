{
  "private": true,
  "name": "react-angular-webpack",
  "version": "0.0.1",
  "description": "Example project with AngularJS, React, and webpack",
  "author": "Matteo Ronchi <matteo.ronchi@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "cross-env NODE_ENV=development webpack --bail --progress --profile --display-error-details --config ./webpack.config.prod.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --history-api-fallback --inline --progress --watch --config ./webpack.config.dev.js"
  },
  "dependencies": {
    "angular": "1.8.3",
    "react": "16.13.0",
    "react-dom": "16.13.0"
  },
  "devDependencies": {
    "@types/angular": "1.6.57",
    "@types/node": "13.7.7",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/webpack": "4.41.7",
    "@typescript-eslint/eslint-plugin": "2.22.0",
    "@typescript-eslint/parser": "2.22.0",
    "copy-webpack-plugin": "5.1.1",
    "cross-env": "7.0.1",
    "css-loader": "3.4.2",
    "file-loader": "5.1.0",
    "fork-ts-checker-webpack-plugin": "4.0.5",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "less": "3.11.1",
    "less-loader": "5.0.0",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "rimraf": "3.0.2",
    "style-loader": "1.1.3",
    "terser-webpack-plugin": "2.3.5",
    "ts-loader": "6.2.1",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslib": "1.11.1",
    "typescript": "3.8.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  }
}
